{
    "bindings": [
        {
            "file": "engine/include/MEEDEngine/MEEDEngine.h",
            "template": "autogen/templates/pyi.j2",
            "output": "editor/Engine/__init__.pyi",
            "dependencies": [
                "engine/include/MEEDEngine",
                "autogen/templates/pyi"
            ],
            "extensions": [".h", ".j2"]
        },
        {
            "file": "engine/include/MEEDEngine/MEEDEngine.h",
            "template": "autogen/templates/binding.j2",
            "output": "engine/tmp/binding.cpp",
            "dependencies": [
                "engine/include/MEEDEngine",
                "autogen/templates/binding"
            ],
            "extensions": [".h", ".j2"]
        }
    ],
    "templates": [
        {
            "file": "meed.code-workspace.in"
        },
        {
            "file": "engine/.vscode/c_cpp_properties.json.in"
        },
        {
            "file": "app/.vscode/c_cpp_properties.json.in"
        },
        {
            "file": "cmake/MEEDConfigure.cmake.in"
        },
        {
            "file": ".vscode/launch.json.in"
        },
        {
            "file": ".vscode/tasks.json.in"
        },
        {
            "file": "config/conf.py.in",
            "noReload": true
        },
        {
            "file": "editor/resources.qrc.in",
            "dependencies": ["editor/assets/icons", "editor/assets/images"],
            "extensions": [".ico", ".png"]
        }
    ]
}
