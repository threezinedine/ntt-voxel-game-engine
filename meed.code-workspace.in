{
    "folders": [
        {
            "name": "cmake",
            "path": "cmake"
        },
        {
            "name": "autogen",
            "path": "autogen",
            "python.defaultInterpreterPath": "{{ BASE_DIR }}/autogen/{{ PYTHON_INTERPRETER }}",
            "python.analysis.extraPaths": [
                "{{ BASE_DIR }}"
            ]
        },
        {
            "name": "app",
            "path": "app",
            "[c]": {
                "editor.defaultFormatter": "xaver.clang-format"
            }
        },
        {
            "name": "engine",
            "path": "engine",
            "[c]": {
                "editor.defaultFormatter": "xaver.clang-format"
            }
        },
        {
            "name": "editor",
            "path": "editor",
            "python.defaultInterpreterPath": "{{ BASE_DIR }}/editor/{{ PYTHON_INTERPRETER }}"
        },
        {
            "name": "config",
            "path": "config"
        },
        {
            "name": "meed-voxel-game-engine",
            "path": "."
        }
    ],
    "settings": {
        "prettier.configPath": "{{BASE_DIR}}/.vscode/.prettierrc"
    }
}
