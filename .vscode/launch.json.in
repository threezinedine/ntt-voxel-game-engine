    "version": "0.2.0",
    "configurations": [
        {
            "name": "Memory Example",
            "type": "cppdbg",
            "request": "launch",
            {% if PLATFORM_DEFINE == "PLATFORM_IS_LINUX" -%}
            "program": "${workspaceFolder}/engine/build/debug/examples/memory",
            {% elif PLATFORM_DEFINE == "PLATFORM_IS_WINDOWS"-%}
            "program": "${workspaceFolder}/engine/build/debug/examples/Debug/memory.exe",
            {% endif -%}
            "args": [],
            {% if PLATFORM_DEFINE == "PLATFORM_IS_LINUX" -%}
            "cwd": "${workspaceFolder}/engine/build/debug/examples",
            {% elif PLATFORM_DEFINE == "PLATFORM_IS_WINDOWS"-%}
            "cwd": "${workspaceFolder}/engine/build/debug/examples/Debug",
            {% endif -%}
            "stopAtEntry": false,
            "preLaunchTask": "Build Engine"
        },
        {
            "name": "Console Example",
            "type": "cppdbg",
            "request": "launch",
            {% if PLATFORM_DEFINE == "PLATFORM_IS_LINUX" -%}
            "program": "${workspaceFolder}/engine/build/debug/examples/console",
