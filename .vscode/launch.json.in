            "program": "${workspaceFolder}/engine/build/debug/examples/Debug/console.exe",
            {% endif -%}
            "args": [],
            {% if PLATFORM_DEFINE == "PLATFORM_IS_LINUX" -%}
            "cwd": "${workspaceFolder}/engine/build/debug/examples",
            {% elif PLATFORM_DEFINE == "PLATFORM_IS_WINDOWS"-%}
            "cwd": "${workspaceFolder}/engine/build/debug/examples/Debug",
            {% endif -%}
            "stopAtEntry": false,
            "preLaunchTask": "Build Engine"
        },
        {
