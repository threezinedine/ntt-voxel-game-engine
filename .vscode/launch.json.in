            {% elif PLATFORM_DEFINE == "PLATFORM_IS_WINDOWS"-%}
            "cwd": "${workspaceFolder}/app/build/debug/Debug",
            {% endif -%}
            "stopAtEntry": false,
            "preLaunchTask": "Build Application"
